body{background:#f2f2f2}.round{border:5px solid #f2f2f2;border-radius:20px;color:#ffe6d4;display:inline-block;font-family:Georgia,serif;font-size:60px;font-weight:700;line-height:100px;width:100px;height:100px;overflow:hidden;text-align:center;text-transform:uppercase;margin:10px}.round:first-child{background-color:#ec7667}.round:nth-child(2){background-color:#fcc490}.round:nth-child(3){background-color:#cad982}.round:nth-child(4){background-color:#6aaead}.round:nth-child(5){background-color:#78dcbd}.round:nth-child(6){background-color:#9191cb}.example{text-align:center}